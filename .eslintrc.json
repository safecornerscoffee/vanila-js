{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["jest"],
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended",
        "plugin:jest/recommended",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "node/no-unpublished-require": "off"
    }
}
